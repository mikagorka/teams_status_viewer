<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.teamsstatus.monitor</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/YOUR_USERNAME/.pyenv/shims/python3</string>
        <string>/Users/YOUR_USERNAME/teams_status_viewer/teams_status_monitor.py</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/YOUR_USERNAME/teams_status_viewer/logs/monitor.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/YOUR_USERNAME/teams_status_viewer/logs/monitor.error.log</string>
    <key>WorkingDirectory</key>
    <string>/Users/YOUR_USERNAME/teams_status_viewer</string>
</dict>
</plist>
